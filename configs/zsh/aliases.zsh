# List of custom Aliases for use with zsh

alias zshconfig="vim ~/.zshrc"
alias wpaper="open ~/.env/wallpaper"
alias uantigen='antigen update; antigen cleanup;'
alias vubrew='brew update -v; brew upgrade -v; brew cask upgrade -v; brew cleanup -v --force --prune=0 --s; brew prune --force --s; brew cask cleanup -v; brew doctor -v'
alias ubrew='brew update; brew upgrade; brew cask upgrade; brew cleanup --force --prune=0 --s; brew prune --force --s; brew cask cleanup; brew doctor;'
alias adventuretime='emacs -batch -1 dunnet'
alias fcl='fortune | cowsay | lolcat'
alias gdb='sudo gdb'
alias pip\ ls='pip list'
alias go-school="cd ~/Desktop/Schooling/UCF/"

