# List of custom Aliases for use with zsh

alias zshconfig="vim ~/.zshrc"
alias wpaper="open ~/.env/wallpaper"

alias adventuretime='emacs -batch -l dunnet'
alias fcl='fortune | cowsay | lolcat'

alias gdb='sudo gdb'
alias 'pip ls'='pip list'
alias vim='nvim'

alias zuc="zplug clear && zplug clean"
alias zup="zplug install && zplug update"
alias zupc="zup && zuc"

alias :q=exit
